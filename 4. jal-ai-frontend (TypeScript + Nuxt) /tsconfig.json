{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    ".nuxt/imports.d.ts",
    ".nuxt/types/**/*.d.ts",
    "shims-vue.d.ts",
    "components/**/*.vue",
    "pages/**/*.vue",
    "layouts/**/*.vue",
    "composables/**/*.ts",
    "plugins/**/*.ts",
    "types/**/*.ts",
    "**/*.ts",
    "**/*.vue"
  ]
}
